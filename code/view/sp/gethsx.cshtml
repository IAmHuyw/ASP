@model IEnumerable<traicay.Models.HangSanXuat>

@{
    ViewBag.Title = "gethsx";
}

<h2>gethsx</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenHang)
        </th>
        <th>
            Số lượng sản phẩm
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenHang)
        </td>
        <td>
            @item.SanPhams.Count
        </td>
    </tr>
}

</table>
